.left-menu {
    -fx-background-color:transparent;
}
.VBox{
    -fx-background-color:transparent;

}
.left-menu:focused{
    -fx-border-color:none;
}
.leftMenu-button {
    -fx-background-color:transparent;
    -fx-padding:50;
}
.leftMenu-button:hover {
    -fx-background-color:rgba(125,125,125,100);
}
.sButton {
    -fx-graphic:url("/pl/police/resources/managePerson.png");
}
.pButton {
    -fx-graphic:url("/pl/police/resources/search.png");
}
.cButton {
    -fx-graphic:url("/pl/police/resources/manageCase.png");
}