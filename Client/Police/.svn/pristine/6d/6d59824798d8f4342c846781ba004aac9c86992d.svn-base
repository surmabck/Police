<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.police.view.menu.cases.CasesListController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="14.0" layoutY="14.0" styleClass="background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox minHeight="0.0" minWidth="0.0">
               <children>
                  <SplitPane dividerPositions="0.5" styleClass="background">
                     <items>
                        <TableView fx:id="casesTable" minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="584.0">
                          <columns>
                            <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
                            <TableColumn fx:id="descriptionColumn" minWidth="0.0" prefWidth="200.0" text="Opis" />
                              <TableColumn fx:id="statusColumn" maxWidth="0.0" minWidth="100.0" prefWidth="100.0" text="Status" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <PieChart fx:id="charte" />
                     </items>
                  </SplitPane>
                  <HBox alignment="BOTTOM_CENTER" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="NEVER">
                     <children>
                        <Button minHeight="50.0" minWidth="200.0" mnemonicParsing="false" styleClass="common-button" text="Edytuj" textAlignment="CENTER" textOverrun="CLIP" />
                        <Button fx:id="showButton" contentDisplay="CENTER" minHeight="50.0" minWidth="200.0" mnemonicParsing="false" onAction="#showButtonClicked" styleClass="common-button" text="Wyswietl" textAlignment="CENTER" textOverrun="CLIP">
                           <HBox.margin>
                              <Insets left="50.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
   <stylesheets>
      <URL value="@../../MainCss.css" />
      <URL value="@CasesList.css" />
   </stylesheets>
</AnchorPane>
